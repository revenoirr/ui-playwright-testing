{
  "name": "demoqa-automation",
  "version": "1.0.0",
  "description": "DemoQA Automation Testing Project",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:chrome": "npx playwright test --project=chromium",
    "test:chrome:headed": "npx playwright test --project=chromium --headed",
    "test:alerts": "npx playwright test alerts.spec.ts --project=chromium",
    "test:alerts:headed": "npx playwright test alerts.spec.ts --project=chromium --headed",
    "test:keyword": "npx playwright test --grep",
    "test:chrome:1920": "npx playwright test --project=chromium --headed",
    "test:debug": "npx playwright test --debug",
    "test:ui": "npx playwright test --ui",
    "test:report": "npx playwright show-report"
  },
  "keywords": [
    "automation",
    "playwright",
    "cucumber",
    "bdd"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@playwright/test": "^1.36.0",
    "dotenv": "^16.3.1",
    "playwright": "^1.36.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@wdio/allure-reporter": "^9.14.0",
    "allure-commandline": "^2.34.0",
    "allure-playwright": "^3.2.2"
  }
